{
  "name": "starter-react",
  "version": "0.0.1",
  "description": "A small scaffold to get you started using React with Webpack",
  "scripts": {
    "start": "npm run browser-serve & npm run api-serve",
    "build": "npm run clean && npm run browser-build && npm run api-build",
    "browser-serve": "parcel serve src/index.html --open",
    "browser-build": "parcel build src/index.html --dist-dir dist/public",
    "api-build": "esbuild ./src/api/*.js --bundle --platform=node --format=cjs --outdir=dist/functions",
    "api-serve": "netlify-lambda serve src/api",
    "clean": "rm -rf dist .parcel-cache"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "got": "^11.3.0",
    "netlify-lambda": "^1.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://www.github.com/colw/cappy"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "esbuild": "^0.5.12",
    "parcel": "^2.0.0-beta.1"
  }
}
