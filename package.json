{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "starter-react",
  "version": "0.0.1",
  "description": "A small scaffold to get you started using React with Webpack",
  "scripts": {
    "process-client": "esbuild ./src/app.js --bundle --outfile=dist/public/bundle.js --minify --loader:.js=jsx '--define:process.env.NODE_ENV=\"production\"' '--define:process.env.COLIN=\"localhost:9000\"'; npm run copy-index; ",
    "process-server": "esbuild ./src/api/*.js --bundle --platform=node --format=cjs --outdir=dist/functions",
    "serve:lambda": "npm run process-client; netlify-lambda serve src/api",
    "copy-index": "mkdir -p dist/public; cp src/index.html dist/public/",
    "build-all": "npm run process-client; npm run process-server",
    "postinstall": "netlify-lambda install src/api"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.3.0",
    "http-proxy-middleware": "^1.0.4",
    "netlify-lambda": "^1.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "recharts": "^1.8.5",
    "request": "^2.88.2"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/starter-react"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "esbuild": "^0.5.12"
  }
}
