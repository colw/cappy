{
  "name": "starter-react",
  "version": "0.0.1",
  "description": "A small scaffold to get you started using React with Webpack",
  "scripts": {
    "start": " npm run api-serve & npm run browser-serve",
    "build": "npm run clean && npm run browser-build && npm run api-build",
    "browser-serve": "node server.js",
    "browser-build": "parcel build --no-cache --no-source-maps src/index.html",
    "api-build": "netlify-lambda build src/api",
    "api-serve": "NODE_ENV=development netlify-lambda serve src/api",
    "clean": "rm -rf dist .parcel-cache",
    "postinstall": "netlify-lambda install src/api",
    "test": "jest -u"
  },
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "apollo-server-lambda": "^2.15.0",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.12",
    "express": "^4.17.1",
    "got": "^11.3.0",
    "http-proxy-middleware": "^1.0.4",
    "i18next": "^19.5.1",
    "netlify-lambda": "^1.6.3",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0",
    "react-i18next": "^11.7.0",
    "recharts": "^1.8.5"
  },
  "engines": {
    "node": "12.x"
  },
  "repository": {
    "url": "https://www.github.com/colw/cappy"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "babel-core": "^6.26.3",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4"
  }
}
